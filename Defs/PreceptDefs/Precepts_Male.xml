  <PreceptDef Class="PTEI.PTEIPreceptDef_Gendered">
    <defName>PTEI_Male_Asexual</defName>
    <issue>TraitEnforcerMale</issue>
    <label>Asexual</label>
    <description>Enforce asexual trait</description>
    <impact>Medium</impact>
    <displayOrderInIssue>1000</displayOrderInIssue>
    <displayOrderInImpact>5000</displayOrderInImpact>
    <gender>Male</gender>
    <comps>
      <li Class="PTEI.PTEIPreceptComp_Standard">
        <trait>Asexual</trait>
        <removeTraits>
            <li>Bisexual</li>
            <li>Gay</li>
        </removeTraits>
      </li>
    </comps>
  </PreceptDef>

